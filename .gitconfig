[user]
	name = Nobuhiko Sawai
	email = nobuhiko.sawai@gmail.com
[color]
	ui = true
[alias]
	ad = add
	adp = add -p
	br = branch
	ci = commit
	ci-am = commit --allow-empty
	co = checkout
	last = log -1 HEAD
	ps = push
	st = status
  sw = switch
  re = restore
	tree = log --graph --pretty='format:%C(yellow)%h%Creset %s %Cgreen(%an)%Creset %Cred%d%Creset'
	tr = log --graph --pretty='format:%C(yellow)%h%Creset %s %Cgreen(%an)%Creset %Cred%d%Creset'
	prb = pull-request --browse
	poh = push origin HEAD
	clean-merged = branch --merged | grep -v '*' | xargs -I % git branch -d %
[core]
	editor = vim
	excludesfile = /Users/nobuhikosawai/.gitignore_global
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
[credential]
	helper = osxkeychain
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true

[commit]
	template = /Users/nobuhikosawai/.stCommitMsg
[url "git@github.com:"]
	insteadOf = https://github.com/
[github]
	user = nobuhikosawai
